{
  "name": "duncantomlin.com",
  "homepage": "https://duncantomlin.com/",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^10.0.4",
    "bulma": "^0.9.1",
    "fabric": "^4.3.0",
    "glob": "^7.1.6",
    "moment": "^2.29.1",
    "next": "^10.0.4",
    "next-seo": "^4.17.0",
    "node-sass": "^4.0.0",
    "prop-types": "^15.7.2",
    "raw.macro": "^0.4.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-favicon": "0.0.18",
    "react-helmet": "^6.1.0",
    "react-markdown": "^5.0.3",
    "react-router-dom": "^5.2.0",
    "webpack": "^4.44.2"
  },
  "scripts": {
    "start": "next",
    "build": "next build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "export": "next export",
    "predeploy": "rm -rf node_modules/.cache && npm run build && npm run export",
    "deploy": "gh-pages -t true -d out && rm -rf out/",
    "analyze": "npm run build && source-map-explorer build/static/js/*.chunk.js"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-nextjs": "^1.0.6",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.6",
    "prettier": "^2.2.1",
    "source-map-explorer": "^2.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
